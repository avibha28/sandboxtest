{
    "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentParameters.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "location": {
            "value": "eastus"
        },
        "extendedLocation": {
            "value": {}
        },
        "virtualNetworkName": {
            "value": "hackathonchallenge{{TEAMCOUNT}}{{NAME}}"
        },
        "resourceGroup": {
            "value": "{{RG}}"
        },
        "addressSpaces": {
            "value": [
                "{{VNETADDRESS}}"
            ]
        },
        "ipv6Enabled": {
            "value": false
        },
        "subnetCount": {
            "value": 1
        },
        "subnet0_name": {
            "value": "default"
        },
        "subnet0_addressRange": {
            "value": "{{SUBNETADDRESS}}"
        },
        "ddosProtectionPlanEnabled": {
            "value": false
        },
        "firewallEnabled": {
            "value": false
        },
        "bastionEnabled": {
            "value": false
        }
    }
}